datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Link {
  id          Int       @id @default(autoincrement())
  url         String
  code        String    @unique
  clicks      Int       @default(0)
  lastClicked DateTime? @default(now())
}





